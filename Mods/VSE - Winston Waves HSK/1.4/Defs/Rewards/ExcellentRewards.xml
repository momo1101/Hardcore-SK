<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentPawnJoins</defName>
    <texPath>UI/Rewards/RewardPawnJoins_Excellent</texPath>
    <label>Pawns join</label>
    <description>A group of 5 pawns worth a minimum of 175 Combat points joins the colony</description>
    <category>Excellent</category>
    <randomPawns>
      <li>
        <minCombatPower>175</minCombatPower>
        <count>5</count>
      </li>
    </randomPawns>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentResourceDrop</defName>
    <texPath>UI/Rewards/RewardResourceDrop_Excellent</texPath>
    <label>Resource drop</label>
    <description>Cargo pods drop 1000 wood, 1000 steel, 60 components, 20 advanced components and 300 plasteel</description>
    <category>Excellent</category>
    <items>
      <li>
        <thing>WoodLog</thing>
        <count>1000</count>
      </li>
      <li>
        <thing>Steel</thing>
        <count>1000</count>
      </li>
      <li>
        <thing>Plasteel</thing>
        <count>300</count>
      </li>
      <li>
        <thing>ComponentIndustrial</thing>
        <count>60</count>
      </li>
      <li>
        <thing>ComponentSpacer</thing>
        <count>20</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentFoodAndMedicineDrop</defName>
    <texPath>UI/Rewards/RewardFoodAndMedicineDrop_Excellent</texPath>
    <label>Food and medicine drop</label>
    <description>Cargo pods drop 120 survival meals, 20 glitterworld medicine and 100 medicine</description>
    <category>Excellent</category>
    <items>
      <li>
        <thing>MealSurvivalPack</thing>
        <count>120</count>
      </li>
      <li>
        <thing>MedicineIndustrial</thing>
        <count>100</count>
      </li>
      <li>
        <thing>MedicineUltratech</thing>
        <count>20</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentFuelDrop</defName>
    <texPath>UI/Rewards/RewardFuelDrop_Excellent</texPath>
    <label>Fuel drop</label>
    <description>Cargo pods drop 1000 chemfuel, 1000 wood and 400 uranium</description>
    <category>Excellent</category>
    <items>
      <li>
        <thing>Chemfuel</thing>
        <count>1000</count>
      </li>
      <li>
        <thing>WoodLog</thing>
        <count>1000</count>
      </li>
      <li>
        <thing>Uranium</thing>
        <count>400</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentWeaponDrop</defName>
    <texPath>UI/Rewards/RewardWeaponsDrop_Excellent</texPath>
    <label>Excellent weapons drop</label>
    <description>Cargo pods 5 random weapons, quality is Excellent</description>
    <category>Excellent</category>
    <randomItems>
      <li>
        <thingCategories>
          <li>Weapons</li>
        </thingCategories>
        <excludeThingCategories>
          <li MayRequire="CETeam.CombatExtended">Ammo</li>
        </excludeThingCategories>
        <quality>Excellent</quality>
        <count>5</count>
      </li>
    </randomItems>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentArmorDrop</defName>
    <texPath>UI/Rewards/RewardArmorDrop_Excellent</texPath>
    <label>Excellent armor drop</label>
    <description>Cargo pods 5 random pieces of armor, quality is Excellent</description>
    <category>Excellent</category>
    <randomItems>
      <li>
        <thingCategories>
          <li>ApparelArmor</li>
        </thingCategories>
        <quality>Excellent</quality>
        <count>5</count>
      </li>
    </randomItems>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentCashDrop</defName>
    <texPath>UI/Rewards/RewardCashDrop_Excellent</texPath>
    <label>Cash</label>
    <description>Cargo pods drop 10000 silver, 500 gold</description>
    <category>Excellent</category>
    <items>
      <li>
        <thing>Silver</thing>
        <count>10000</count>
      </li>
      <li>
        <thing>Gold</thing>
        <count>500</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_DelayBy15</defName>
    <texPath>UI/Rewards/RewardDelayWave_Excellent</texPath>
    <label>Delay next wave</label>
    <description>The next wave is delayed by 15 days</description>
    <category>Excellent</category>
    <waveModifier>
      <delayBy>15</delayBy>
    </waveModifier>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_WargDrop</defName>
    <texPath>UI/Rewards/RewardWargsDrop_Excellent</texPath>
    <label>Wargs drop</label>
    <description>Cargo pod drops 10 tamed wargs</description>
    <category>Excellent</category>
    <pawns>
      <li>
        <pawnkind>Warg</pawnkind>
        <count>10</count>
      </li>
    </pawns>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentSkillBoost</defName>
    <texPath>UI/Rewards/RewardSkillBoost_Excellent</texPath>
    <label>Skill boost</label>
    <description>Improve every skill of every pawn under your control by 3</description>
    <category>Excellent</category>
    <boostSkillBy>3</boostSkillBy>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentReseachBoost</defName>
    <texPath>UI/Rewards/RewardResearchBoost_Excellent</texPath>
    <label>Research boost</label>
    <description>Instantly research 8 random research projects you meet prerequisites for</description>
    <category>Excellent</category>
    <unlockXResearch>8</unlockXResearch>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentArchotechArtifactsDrop</defName>
    <texPath>UI/Rewards/RewardArchotechArtifactDrop_Excellent</texPath>
    <label>Archotech artifacts drop</label>
    <description>Receive 8 random archotech items (can be minified structures)</description>
    <category>Excellent</category>
    <randomItems>
      <li>
        <thingCategories>
          <li>Artifacts</li>
        </thingCategories>
        <count>8</count>
      </li>
    </randomItems>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_TargetersDrop</defName>
    <texPath>UI/Rewards/RewardTargetersDrop_Excellent</texPath>
    <label>Targeters drop</label>
    <description>Receive an orbital beam targeter, orbital bombardment targeter and tornado generator</description>
    <category>Excellent</category>

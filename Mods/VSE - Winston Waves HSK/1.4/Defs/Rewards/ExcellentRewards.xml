      <li>
        <thing>Ambrosia</thing>
        <count>100</count>
      </li>
      <li>
        <thing>PsychiteTea</thing>
        <count>100</count>
      </li>
      <li>
        <thing>Beer</thing>
        <count>200</count>
      </li>
      <li>
        <thing>SmokeleafJoint</thing>
        <count>200</count>
      </li>
      <li>
        <thing>Flake</thing>
        <count>200</count>
      </li>
      <li>
        <thing>Yayo</thing>
        <count>200</count>
      </li>
      <li>
        <thing>WakeUp</thing>
        <count>200</count>
      </li>
      <li>
        <thing>GoJuice</thing>
        <count>200</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_ExcellentProstheticsDrop</defName>
    <texPath>UI/Rewards/RewardProsthethicsDrop_Excellent</texPath>
    <label>Prosthetics drop</label>
    <description>Cargo pod drops random 10 bionic prosthetics</description>
    <category>Excellent</category>
    <randomItems>
      <li>
        <thingCategories>
          <li>BodyPartsBionic</li>
        </thingCategories>
        <count>10</count>
        <quality MayRequire="rebelrabbit.qualitybionics">Excellent</quality>
      </li>
    </randomItems>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_MechSerumHealerDrop</defName>
    <texPath>UI/Rewards/RewardHealerSerumDrop_Excellent</texPath>
    <label>Healer serum drop</label>
    <description>Cargo pod drops 5 Mechanoid healer serum</description>
    <category>Excellent</category>
    <items>
      <li>
        <thing>MechSerumHealer</thing>
        <count>5</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_RandomLegendaryDrop</defName>
    <texPath>UI/Rewards/RewardRandom_Legendary</texPath>
    <label>Random Legendary reward</label>
    <description>Instead of picking up a specific Excellent-level reward, obtain a completely random Legendary-level reward</description>
    <category>Excellent</category>
    <sendRewardOf>Legendary</sendRewardOf>
  </VSEWW.RewardDef>
</Defs>
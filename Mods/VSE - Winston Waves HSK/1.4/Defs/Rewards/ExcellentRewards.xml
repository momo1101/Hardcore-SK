      <li>
        <thing>OrbitalTargeterPowerBeam</thing>
        <count>1</count>
      </li>
      <li>
        <thing>OrbitalTargeterBombardment</thing>
        <count>1</count>
      </li>
      <li>
        <thing>TornadoGenerator</thing>
        <count>1</count>
      </li>
    </items>
  </VSEWW.RewardDef>


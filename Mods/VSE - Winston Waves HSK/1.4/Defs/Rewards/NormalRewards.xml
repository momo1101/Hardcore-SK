<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <VSEWW.RewardDef>
    <defName>VSEWW_NormalPawnJoins</defName>
    <texPath>UI/Rewards/RewardPawnJoins_Normal</texPath>
    <label>Pawn joins</label>
    <description>A single pawn worth minimum 50 and up to 125 Combat points joins the colony</description>
    <category>Normal</category>
    <randomPawns>
      <li>
        <minCombatPower>50</minCombatPower>
        <maxCombatPower>125</maxCombatPower>
        <count>1</count>
      </li>
    </randomPawns>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalResourceDrop</defName>
    <texPath>UI/Rewards/RewardResourceDrop_Normal</texPath>
    <category>Normal</category>
    <label>Resource drop</label>
    <description>Cargo pods drop 250 wood, 250 steel, 10 components</description>
    <items>
      <li>
        <thing>WoodLog</thing>
        <count>250</count>
      </li>
      <li>
        <thing>Steel</thing>
        <count>250</count>
      </li>
      <li>
        <thing>ComponentIndustrial</thing>
        <count>10</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalComponentDrop</defName>
    <texPath>UI/Rewards/RewardComponentsDrop_Normal</texPath>
    <label>Component drop</label>
    <description>Cargo pods drop 20 components, 5 advanced components</description>
    <category>Normal</category>
    <items>
      <li>
        <thing>ComponentIndustrial</thing>
        <count>20</count>
      </li>
      <li>
        <thing>ComponentSpacer</thing>
        <count>5</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalFoodAndMedicineDrop</defName>
    <texPath>UI/Rewards/RewardFoodAndMedicineDrop_Normal</texPath>
    <label>Food and medicine drop</label>
    <description>Cargo pods drop 30 survival meals and 30 medicine</description>
    <category>Normal</category>
    <items>
      <li>
        <thing>MealSurvivalPack</thing>
        <count>30</count>
      </li>
      <li>
        <thing>MedicineIndustrial</thing>
        <count>30</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_UraniumDrop</defName>
    <texPath>UI/Rewards/RewardUraniumDrop_Normal</texPath>
    <label>Uranium drop</label>
    <description>Cargo pods drop 100 uranium</description>
    <category>Normal</category>
    <items>
      <li>
        <thing>Uranium</thing>
        <count>100</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalFuelDrop</defName>
    <texPath>UI/Rewards/RewardFuelDrop_Normal</texPath>
    <label>Fuel drop</label>
    <description>Cargo pods drop 500 chemfuel</description>
    <category>Normal</category>
    <items>
      <li>
        <thing>Chemfuel</thing>
        <count>500</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_PlasteelDrop</defName>
    <texPath>UI/Rewards/RewardPlasteelDrop_Normal</texPath>
    <label>Plasteel drop</label>
    <description>Cargo pods drop 100 plasteel</description>
    <category>Normal</category>
    <items>
      <li>
        <thing>Plasteel</thing>
        <count>100</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalWeaponsDrop</defName>
    <texPath>UI/Rewards/RewardWeaponsDrop_Normal</texPath>
    <label>Normal weapons drop</label>
    <description>Cargo pods 3 random weapons, quality is normal</description>
    <category>Normal</category>
    <randomItems>
      <li>
        <thingCategories>
          <li>Weapons</li>
        </thingCategories>
        <excludeThingCategories>
          <li MayRequire="CETeam.CombatExtended">Ammo</li>
        </excludeThingCategories>
        <quality>Normal</quality>
        <count>3</count>
      </li>
    </randomItems>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalCashDrop</defName>
    <texPath>UI/Rewards/RewardCashDrop_Normal</texPath>
    <label>Cash</label>
    <description>Cargo pods drop 3000 silver</description>
    <category>Normal</category>
    <items>
      <li>
        <thing>Silver</thing>
        <count>3000</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_CaravanArrival</defName>
    <texPath>UI/Rewards/RewardCaravanArrival_Normal</texPath>
    <label>Caravan arrival</label>
    <description>A random caravan arrives</description>
    <category>Normal</category>
    <incidentDef>TraderCaravanArrival</incidentDef>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_DelayBy3</defName>
    <texPath>UI/Rewards/RewardDelayWave_Normal</texPath>
    <label>Delay next wave</label>
    <description>Next wave is delayed by 3 days</description>
    <category>Normal</category>
    <waveModifier>
      <delayBy>3</delayBy>
    </waveModifier>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalAnimalDrop</defName>
    <texPath>UI/Rewards/RewardAnimalDrop_Normal</texPath>
    <label>Animal drop</label>
    <description>Cargo pod drops 2 random, tamed animals</description>
    <category>Normal</category>
    <randomPawns>
      <li>
        <intelligence>Animal</intelligence>
        <count>2</count>
      </li>
    </randomPawns>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalTextileDrop</defName>
    <texPath>UI/Rewards/RewardTextileDrop_Normal</texPath>
    <label>Textile drop</label>
    <description>Cargo pod drops 400 random pieces of leather or textile</description>
    <category>Normal</category>
    <randomItems>
      <li>
        <thingCategories>
          <li>Leathers</li>
          <li>Textiles</li>
        </thingCategories>
        <count>400</count>
      </li>
    </randomItems>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_MiniturretDrop</defName>
    <texPath>UI/Rewards/RewardMiniTurretDrop_Normal</texPath>
    <label>Miniturret drop</label>
    <description>Cargo pod drops 3 minified mini-turrets</description>
    <category>Normal</category>
    <items>
      <li>
        <thing>Turret_MiniTurret</thing>
        <count>3</count>
      </li>
    </items>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalReseachBoost</defName>
    <texPath>UI/Rewards/RewardResearchBoost_Normal</texPath>
    <label>Research boost</label>
    <description>Instantly research 3 random research projects you meet prerequisites for</description>
    <category>Normal</category>
    <unlockXResearch>3</unlockXResearch>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_NormalProstheticsDrop</defName>
    <texPath>UI/Rewards/RewardProsthethicsDrop_Normal</texPath>
    <label>Prosthetics drop</label>
    <description>Cargo pod drops 5 random low-level prosthetics</description>
    <category>Normal</category>
    <randomItems>
      <li>
        <thingCategories>
          <li>BodyPartsProsthetic</li>
        </thingCategories>
        <count>5</count>
        <quality MayRequire="rebelrabbit.qualitybionics">Normal</quality>
      </li>
    </randomItems>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_RandomGoodDrop</defName>
    <texPath>UI/Rewards/RewardRandom_Good</texPath>
    <label>Random Good reward</label>
    <description>Instead of picking up a specific Normal-level reward, obtain a completely random Good-level reward</description>
    <category>Normal</category>
    <sendRewardOf>Good</sendRewardOf>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_MeteoriteImpact</defName>
    <texPath>UI/Rewards/RewardMeteoriteImpact_Normal</texPath>
    <label>Meteorite impact</label>
    <description>Meteorite impact</description>
    <category>Normal</category>
    <incidentDef>MeteoriteImpact</incidentDef>
  </VSEWW.RewardDef>

  <VSEWW.RewardDef>
    <defName>VSEWW_PsychicSoothe</defName>
    <texPath>UI/Rewards/RewardPsychicSoothe_Normal</texPath>
    <label>Psychic Soothe</label>
    <description>Psychic soothe</description>
    <category>Normal</category>
    <incidentDef>PsychicSoothe</incidentDef>
  </VSEWW.RewardDef>
</Defs>
<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Bad Hygiene</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/NeedDef[defName="Bladder"]/Exemptions/BodyDefs</xpath>
					<value>
						<li>Antis</li>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/NeedDef[defName="Bladder"]/Exemptions/RaceDefs</xpath>
					<value>
						<li>Crashbug</li>
						<li>Mosquito</li>
						<li>Fly</li>
						<li>Flea</li>
						<li>Cockroach</li>
						<li>CropEaterInsect</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
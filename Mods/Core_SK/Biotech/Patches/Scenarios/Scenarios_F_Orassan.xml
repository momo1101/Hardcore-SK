<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ScenarioDef[defName="TwoTails"]/scenario/parts/li[@Class="ScenPart_ConfigPage_ConfigureStartingPawns"]</xpath>
		<value>
			<li Class="ScenPart_ConfigPage_ConfigureStartingPawns_Xenotypes">
				<def>ConfigurePawnsXenotypes</def>
				<pawnChoiceCount>8</pawnChoiceCount>
				<customSummary>Start with two Orassans.</customSummary><!--
				<overrideKinds>
					<li>
						<xenotype>Baseline_Orassan</xenotype>
						<pawnKind>Orassan</pawnKind>
					</li>
					<li>
						<xenotype>Baseline_Orassan</xenotype>
						<pawnKind>OrassanColonist</pawnKind>
					</li>
				</overrideKinds>-->
				<xenotypeCounts>
					<li>
						<xenotype>Baseline_Orassan</xenotype>
						<count>2</count>
						<description>non-baby orassan</description>
						<requiredAtStart>true</requiredAtStart>
						<allowedDevelopmentalStages>Adult, Child</allowedDevelopmentalStages>
					</li>
				</xenotypeCounts>
			</li>
		</value>
	</Operation>

</Patch>	
<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="SK_ResourceBase">
		<defName>MedicineHerbal</defName>
		<label>Raw herbal medicine</label>
		<description>A pack of herbal concoctions extracted from the healroot plant. It is less potent than industrial pharmaceuticals, but much better than nothing.</description>
		<thingClass>Medicine</thingClass>
		<healthAffectsPrice>false</healthAffectsPrice>
		<possessionCount>4</possessionCount>
		<graphicData>
			<texPath>Things/Item/Resource/RawXerigium</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<MarketValue>8</MarketValue>
			<Mass>0.1</Mass>
			<Bulk>0.1</Bulk>
			<Flammability>1.3</Flammability>
			<DeteriorationRate>15</DeteriorationRate>
			<MedicalPotency>0.30</MedicalPotency>
			<MedicalQualityMax>0.60</MedicalQualityMax>
		</statBases>
		<stackLimit>75</stackLimit>
		<thingCategories>
			<li>PlantMatter</li>
			<li>Medicine</li>
		</thingCategories>
		<tickerType>Rare</tickerType>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<allowedArchonexusCount>20</allowedArchonexusCount>
		<thingSetMakerTags>
			<li>HSKhumanitarianSuppliesTierOne</li>
		</thingSetMakerTags>
	</ThingDef>


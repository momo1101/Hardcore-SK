	<ThingDef ParentName="LWM_DeepStorage">
		<defName>HSK_FishTrap</defName>
		<label>fish trap</label>
		<thingClass>SK.Building_FishTrap</thingClass>
		<description>A trap designed to catch small fish. Catches fish slowly over time, however providing some fish bait will speed it up considerably.\n\nIt is recommended to put multiple fish trap at least 18 cells apart from each other.\nThere are minor penalties for being close.</description>
		<graphicData>
		  <texPath>Things/Building/Fishing/ShellfishTrap</texPath>
		  <graphicClass>Graphic_Single</graphicClass>
		  <drawRotated>false</drawRotated>
		  <drawSize>(1,1)</drawSize>
		</graphicData>
		<blockLight>false</blockLight>
		<!--<placingDraggableDimensions>2</placingDraggableDimensions>-->
		<altitudeLayer>Building</altitudeLayer>
		<rotatable>false</rotatable>
		<statBases>
		  <WorkToBuild>250</WorkToBuild>
		  <Mass>5</Mass>
		</statBases>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>35</costStuffCount>
		<costList>
			<Wire>2</Wire>
		</costList>
		<!--<constructEffect>ConstructWood</constructEffect>-->
		<useHitPoints>true</useHitPoints>
		<size>(1,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<tickerType>Normal</tickerType>
		<pathCost>60</pathCost>
		<terrainAffordanceNeeded>ShallowWater</terrainAffordanceNeeded>
		<placeWorkers>
		  <li>SK.PlaceWorker_FishTrap</li>
		</placeWorkers>
		<category>Building</category>
		<soundImpactDefault>BulletImpact_Wood</soundImpactDefault>
		<selectable>true</selectable>
		<drawerType>MapMeshAndRealTime</drawerType>
		<repairEffect>Repair</repairEffect>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<filthLeaving>Filth_RubbleBuilding</filthLeaving>
		<minifiedDef>MinifiedThing</minifiedDef>
		<building>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<repairable>False</repairable>
			<destroySound>BuildingDestroyed_Soft_Small</destroySound>
			<fixedStorageSettings>
				<priority>Unstored</priority>
				<filter>
					<disallowedCategories>
						<li>Root</li>
					</disallowedCategories>
					<specialFiltersToDisallow>
						<li>AllowRotten</li>
						<li>AllowLargeCorpses</li>
					</specialFiltersToDisallow>
				</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
				<priority>Unstored</priority>
				<filter>
					<disallowedCategories>
						<li>Root</li>
					</disallowedCategories>
					<specialFiltersToDisallow>
						<li>AllowRotten</li>
						<li>AllowLargeCorpses</li>
					</specialFiltersToDisallow>
				</filter>
			</defaultStorageSettings>
		  <artificialForMeditationPurposes>false</artificialForMeditationPurposes>
		</building>
		<thingCategories>
		  <li>BuildingsProduction</li>
		</thingCategories>
		<researchPrerequisites>
			<li>Fish_A1</li>
		</researchPrerequisites>
		<comps>
			<li Class="LWM.DeepStorage.Properties" >
				<maxNumberStacks>3</maxNumberStacks>
				<minTimeStoringTakes>15</minTimeStoringTakes>
				<additionalTimeEachStack>30</additionalTimeEachStack>
				<additionalTimeEachDef>15</additionalTimeEachDef>
				<showContents>false</showContents>
				<overlayType>SumOfItemsPerCell</overlayType>
			</li>
			<li Class="SK.CompProperties_RefuelableRare">
				<fuelConsumptionRate>5.0</fuelConsumptionRate>
				<fuelCapacity>5.0</fuelCapacity>
				<fuelConsumptionPerTickInRain>0.01</fuelConsumptionPerTickInRain>
				<fuelFilter>
					<thingDefs>
						<li>FishingBait</li>
					</thingDefs>
				</fuelFilter>
				<initialFuelPercent>0</initialFuelPercent>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
			</li>
			<li>
				<compClass>CompQuality</compClass>
			</li>
		</comps>
	</ThingDef>

</Defs>

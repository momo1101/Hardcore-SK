<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/ThingDef[@Name="SK_BasePawn"]/comps</xpath>
        <value>
            <li>
                <compClass>ArmorRacks.ThingComps.ArmorRackUseCommandComp</compClass>
            </li>
        </value>
    </Operation>
	
	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Skynet</li>
	</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="BasePawnSkynet"]/comps</xpath>
					<value>
						<li>
							<compClass>ArmorRacks.ThingComps.ArmorRackUseCommandComp</compClass>
						</li>
					</value>
				</li>
			
			</operations>
		</match>
	</Operation>
	
</Patch>
<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<FactionDef ParentName="SK_FactionBase">
		<defName>Darkness</defName>
		<label>Darkspawn</label>
		<description>These swift, merciless creatures came from the darkest voids of the galaxy. By being born from shadows, they abhor light. With a icy cold bite they seek to extinguish the bright flames of life.</description>
		<pawnSingular>void</pawnSingular>
		<pawnsPlural>voids</pawnsPlural>
		<requiredCountAtGameStart>1</requiredCountAtGameStart>
		<fixedName>darkness</fixedName>
		<factionIconPath>World/WorldObjects/Expanding/Insects</factionIconPath>
		<colorSpectrum>
			<li>(0.44, 0.41, 0.32)</li>
			<li>(0.61, 0.58, 0.49)</li>
			<li>(0.60, 0.49, 0.36)</li>
		</colorSpectrum>
		<raidCommonalityFromPointsCurve>
			<points>
				<li>(0, 0)</li>
			</points>
		</raidCommonalityFromPointsCurve>
		<humanlikeFaction>false</humanlikeFaction>
		<techLevel>Neolithic</techLevel>
		<hidden>true</hidden>
		<autoFlee>false</autoFlee>
		<canUseAvoidGrid>false</canUseAvoidGrid>
		<permanentEnemy>true</permanentEnemy>
		<settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
		<maxConfigurableAtWorldCreation>1</maxConfigurableAtWorldCreation>
		<configurationListOrderPriority>100</configurationListOrderPriority>
	</FactionDef>

</Defs>
			<partEfficiency>1.05</partEfficiency>
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
		<spawnThingOnRemoved>ANN_Greenthumb</spawnThingOnRemoved>
		<descriptionHyperlinks>
			<ThingDef>ANN_Greenthumb</ThingDef>
		</descriptionHyperlinks>
	</HediffDef>

